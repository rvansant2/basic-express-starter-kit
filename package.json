{
  "name": "basic-express-starter-kit",
  "version": "1.0.0",
  "description": "Simple nodejs starter kit",
  "main": "index.js",
  "private": true,
  "keywords": [
    "nodejs",
    "expressjs",
    "babel",
    "eslint",
    "prettier",
    "mocha"
  ],
  "author": "rvansant2",
  "license": "MIT",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon dist/app.js",
    "build": "babel src -d dist",
    "watch": "babel src -d dist --watch",
    "test": "mocha --compilers js:babel-core/register src/test",
    "lint": "eslint --fix src",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,md}": [
      "eslint --fix src",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "^4.1.1",
    "express": "^4.15.5",
    "jade": "~1.11.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.2",
    "package.json": "^2.0.1",
    "serve-favicon": "^2.4.5",
    "stable": "^0.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "chai": "^4.1.2",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^7.1.1",
    "prettier": "^2.0.2"
  },
  "prettier/prettier": [
    "error",
    {
      "singleQuote": true
    }
  ]
}
